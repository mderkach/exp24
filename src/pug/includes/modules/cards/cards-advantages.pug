block cardsAdvantages

.cards__advantages-outer
  each card in cardsAdvantages
    .cards__advantages(class=advantagesClass)
      img.cards__advantages--img(src=card.img, alt='')
      .cards__advantages--body
        p.cards__advantages--header
          | #{card.name}
        p.cards__advantages--descr
          | #{card.descr}
